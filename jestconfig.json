{
    "collectCoverage": true,
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "moduleFileExtensions": [
        "ts",
        "js",
        "tsx",
        "jsx",
        "json",
        "node"
    ],
    "rootDir": ".",
    "testMatch": [
        "**/__tests__/**/**/?(*.)+(spec|test).+(ts|tsx|js)"
    ]
}