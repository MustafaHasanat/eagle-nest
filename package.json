{
  "name": "@mustafa-alhasanat/eagle-nest",
  "version": "0.0.0-development",
  "description": "Build Nest.js common blocks and files insanely faster!",
  "main": "./dist/index.js",
  "type": "module",
  "bin": {
    "eaglenest": "./dist/index.js"
  },
  "scripts": {
    "test": "jest",
    "dev": "npx nodemon",
    "commit": "git-cz",
    "start": "npm run build && node dist/index.js",
    "build": "rimraf ./dist && tsc",
    "semantic-release": "semantic-release --branches main"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MustafaHasanat/eagle-nest.git"
  },
  "keywords": [
    "nest",
    "backend",
    "blocks",
    "api",
    "cli",
    "npmtool"
  ],
  "author": "Mustafa Alhasanat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MustafaHasanat/eagle-nest/issues"
  },
  "homepage": "https://github.com/MustafaHasanat/eagle-nest#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "exports": {
    ".": "./index.ts"
  },
  "dependencies": {
    "@inquirer/checkbox": "^1.4.0",
    "@inquirer/confirm": "^2.0.14",
    "@inquirer/input": "^1.2.13",
    "@inquirer/select": "^1.3.0",
    "@nestjs/common": "^10.2.9",
    "@nestjs/platform-express": "^10.2.9",
    "@nestjs/swagger": "^7.1.16",
    "class-transformer": "^0.5.1",
    "commander": "^11.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "express": "^4.18.2",
    "figlet": "^1.7.0",
    "fs": "^0.0.1-security",
    "htmlparser2": "^9.0.0",
    "inquirer": "^9.2.11",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.6",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/figlet": "^1.5.7",
    "@types/htmlparser2": "^3.10.6",
    "@types/inquirer": "^9.0.6",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
