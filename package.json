    "@types/jest": "^29.5.7",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.10",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.6",
